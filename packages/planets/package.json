{
  "name": "swapi-apollo-planets",
  "version": "0.0.1",
  "description": "The Planets module for Apollo GraphQL SWAPI",
  "main": "./dist/index.js",
  "scripts": {
    "build": "tsc",
    "lint": "tslint src/**/*",
    "start": "npm run build && node ./dist/index.js",
    "dev": "nodemon ./dist/index.js",
    "test:record": "npm run build && VCR_MODE=record mocha ./dist/index.test",
    "test": "npm run build && VCR_MODE=playback mocha ./dist/index.test"
  },
  "keywords": [
    "Apollo-Stack",
    "GraphQL"
  ],
  "author": "Nick Nance",
  "license": "ISC",
  "dependencies": {
    "@creditkarma/graphql-loader": "^0.7.0",
    "dataloader": "^1.3.0",
    "debug": "^3.1.0",
    "graphql-tools": "^2.18.0",
    "request": "^2.83.0",
    "swapi-apollo-types": "^0.0.1"
  },
  "peerDependencies": {
    "graphql": "^0.11.0"
  }
}
